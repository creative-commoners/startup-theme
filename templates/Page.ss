<!doctype html>
<html lang="$ContentLocale">
<head>
    <%-- Required meta tags --%>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    $MetaTags(false)

    <%-- Favicons --%>
    <link rel="apple-touch-icon" sizes="180x180" href="$resourceURL('themes/startup/favicons/apple-touch-icon.png')">
    <link rel="icon" type="image/png" sizes="32x32" href="$resourceURL('themes/startup/favicons/favicon-32x32.png')">
    <link rel="icon" type="image/png" sizes="16x16" href="$resourceURL('themes/startup/favicons/favicon-16x16.png')">
    <link rel="manifest" href="$resourceURL('themes/startup/favicons/site.webmanifest')">
    <link rel="mask-icon" href="$resourceURL('themes/startup/favicons/safari-pinned-tab.svg')" color="#051e2d">
    <link rel="shortcut icon" href="$resourceURL('themes/startup/favicons/favicon.ico')">
    <meta name="msapplication-TileColor" content="#051e2d">
    <meta name="msapplication-config" content="$resourceURL('themes/startup/favicons/browserconfig.xml')">
    <meta name="theme-color" content="#ffffff">

    <title><% if $MetaTitle %>$MetaTitle<% else %>$Title<% end_if %> | $SiteConfig.Title</title>
</head>
<body>
    <div>Page template forthcoming</div>
</body>
</html>